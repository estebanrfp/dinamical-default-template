@media (max-width: 650px) {
    .hero h1 {
        font-size: 1.2rem;
        margin: 0 0 40px;
    }
    .hero p {
        display: none;
    }
    .tab-content h3 {
        margin: 0 0 12px;
        font-size: 1rem;
    }
    .hp-container {
        width: 100%;
    }
    .seamless .bloggers img {
        width: 50%;
        padding: .5rem;
    }
}


@media screen and (max-width: 400px){
    .showcase {
        padding: 1rem 0 48px;
    }
    .hp-container {
        margin: 0 auto;
    }
    .tab-content h3 {
        margin: 0 0 12px;
        font-size: 1rem;
    }
    .tab-content img {
        display: block;
        width: 100%;
    }
}