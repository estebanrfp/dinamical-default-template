.showcase {
    padding: 60px 0 48px;
    background: #22303d;
}

.tab-content {
    /* padding: 48px 0; */
    margin: 1rem;
}
.tab-content h3 {
    margin: 0 0 12px;
    color: #CCC;
    font-size: 20px;
    font-weight: 700;
    text-transform: uppercase;
    background: #34495e;
    padding: 1rem;
}
.tab-content h3 .note1 {
    background:red;
}
.tab-content p {
    color: #999;
    text-align: left;
    padding: .5rem;;
}
.tab-content img {
    display: block;
    width: 50%;
    min-width: 200px
    float: right;
    padding: 1rem;
}

.tab-content .row {
    display: table;
}
.tab-content .row .column {
    display: table-cell;
    width: 20%;
    padding: 0 0 0 24px;
    vertical-align: middle;
}
.tab-content .row .column:first-child {
    width: 40%;
    padding: 0;
}

.features,
.abtest,
.responsive {
    background: rgba(255, 255, 255, 0.99);
    padding: 1rem 0;
}
.features h2,
.abtest h2,
.responsive h2 {
    // margin: 30px 0 24px;
    color: #546577;
}
.features h5,
.abtest h5,
.responsive h5 {
    margin: 0 0 48px;
}
.features p,
.abtest p,
.responsive p {
    color: #999;
    //margin: 0 0 48px;
    text-align: center;
    text-transform: initial;
}
.features img,
.abtest img,
.responsive img {
    display: block;
    width: 100%;
    margin: 0 auto;
}

